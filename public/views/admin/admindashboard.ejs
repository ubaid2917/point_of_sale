/* Apply styling to the content sections */
.main-section {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    height: 80vh;
    /* background-color: #f0f0f0; */
}

/* Apply styling to the content sections */
.content-section, .content-section1 {
    background-color: #ffffff;
    border-radius: 10px;
    padding: 20px;
    border: 2px solid grey;
    box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.2);
    text-align: center;
    max-width: 400px;
    margin-bottom: 20px;
}

/* Style for the welcome message */
.content-section h2 {
    font-size: 24px;
    color: #333333;
}

/* Style for the paragraph */
.content-para {
    font-size: 16px;
    line-height: 1.5;
    color: #333333;
}
   </style>
